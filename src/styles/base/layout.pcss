@import "generic.reset.pcss";

@import "trumps.utilities.pcss";

@import "settings.custom-properties.pcss";

@import "settings.custom-media.pcss";

@import "settings.fonts.pcss";

@import "inspections.pcss";

*,
*::before,
*::after {
	box-sizing: border-box;
}

.js-focus-visible :focus:not(.focus-visible) {
	outline: none;
}

html,
body {
	font-size: 16px;
	font-family: "open-sans", sans-serif;
}
